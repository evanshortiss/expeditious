{
  "name": "expeditious",
  "version": "1.0.1",
  "description": "caching interface to simplify interaction with multiple storage engines",
  "main": "lib/cache.js",
  "scripts": {
    "test": "npm run lint && npm run unit && npm run coverage",
    "lint": "npm run eslint && npm run linelint",
    "eslint": "eslint $(ls lib/*.js)",
    "linelint": "linelint $(ls lib/*.js)",
    "unit": "mocha test/",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls",
    "coverage": "nyc mocha test/ && nyc report --reporter=lcov"
  },
  "keywords": [
    "expeditious",
    "caching",
    "cache",
    "api"
  ],
  "author": "Evan Shortiss",
  "license": "MIT",
  "dependencies": {
    "debug": "~2.6.9",
    "safejson": "~1.0.1",
    "verror": "~1.6.1"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.13.0",
    "eslint": "~3.15.0",
    "linelint": "~1.0.1",
    "mocha": "~2.5.3",
    "mocha-lcov-reporter": "~1.3.0",
    "nyc": "~6.6.1",
    "proxyquire": "~1.7.9",
    "sinon": "~1.17.4"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
